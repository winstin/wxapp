<block wx:if="{{$taroCompReady}}">
    <view class="{{styles.user}}">
        <view class="{{styles.userInfo}}" bindtap="manageCard">
            <view class="{{styles.info}}">
                <image class="{{styles.avatar}}" src="{{userInfo.avatarUrl}}"></image>
                <view class="{{styles.lr}}">
                    <view class="{{styles.lrTop}}">
                        <view class="{{styles.name}}">{{userInfo.nickName}}</view>
                    </view>
                    <view class="{{styles.phone}}">{{'1074 2210 2177 51023'}}
                        <image class="{{styles.itemIcon}}" src="{{cardIcon}}"></image>
                    </view>
                </view>
            </view>
        </view>
        <view class="{{styles.cardtop}}" bindtap="manageCard">
            <view class="{{styles.cardL}}">
                <view class="{{styles.itemTitle}}">会员积分</view>
            </view>
            <view class="{{styles.cardL}}">
                <view class="{{styles.vipNumber}}">2000</view>
                <image class="{{styles.arrow}}" src="{{arrowIcon}}" bindtap="goInfo"></image>
            </view>
        </view>
        <list-item compid="{{$compid__187}}"></list-item>
        <list-item compid="{{$compid__188}}"></list-item>
        <list-item compid="{{$compid__189}}"></list-item>
        <list-item compid="{{$compid__190}}"></list-item>
        <list-item compid="{{$compid__191}}"></list-item>
        <list-item compid="{{$compid__192}}"></list-item>
        <list-item compid="{{$compid__193}}"></list-item>
        <list-item compid="{{$compid__194}}"></list-item>
        <list-item compid="{{$compid__195}}"></list-item>
    </view>
</block>